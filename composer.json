{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "4.2.*",
		"guzzlehttp/guzzle": "4.*",
		"indatus/dispatcher": "1.4.*@dev"
	},
	"require-dev": {
	    "way/generators": "~2.0"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php5-latest artisan clear-compiled",
			"php5-latest artisan optimize"
		],
		"post-update-cmd": [
			"php5-latest artisan clear-compiled",
			"php5-latest artisan optimize"
		],
		"post-create-project-cmd": [
			"php5-latest artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
